<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="mysqlgenerator"  targetRuntime="MyBatis3">
        <plugin type="com.hyht.plugins.mybatis.MySQLLimitPlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <commentGenerator type="org.mybatis.generator.internal.CustomeCommentGenerator">
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://rm-uf6ak4u0270j42slto.mysql.rds.aliyuncs.com:3306/hyht_demo?useUnicode=true&amp;characterEncoding=UTF-8"
                        userId="hyht_demo" password="hyht@demo@l23k12">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <javaModelGenerator targetPackage="com.isuwang.web.sso.dbmodel"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
            <!--<property name="rootClass" value="com.hyht.framework.oc.comm.IdEntity"/>-->
        </javaModelGenerator>
        <!-- 生成映射文件的包名和位置-->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.isuwang.web.sso.dbmapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="userinfo" domainObjectName="User">
            <generatedKey column="id" sqlStatement=" select replace(uuid(),'-','')" />
        </table>
    </context>
</generatorConfiguration>